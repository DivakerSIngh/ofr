                            <div class="row">
                                <div class="col-md-5  col-xs-5">Points Redeemed</div>
                                <div class="col-md-1  col-xs-2">:</div>
                                <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @GrandTotal</strong></div>
                            </div>
                            <hr>

                            <div class="row">
                                <div class="col-md-5  col-xs-5">Services & Taxes Paid </div>
                                <div class="col-md-1 col-xs-1">:</div>
                                <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @finalTaxes</strong></div>
                            </div>
                        }
                        else
                        {
                            <div class="row">
                                <div class="col-md-5  col-xs-5">Services & Taxes @(Model.objBooking.sExtra3 == "Redeem" ? Html.Raw("Paid") : null) </div>
                                <div class="col-md-1 col-xs-1">:</div>
                                <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @finalTaxes</strong></div>
                            </div>

                            if (Model.objBooking.ccType == "RR")
                            {
                                <div class="row">
                                    <div class="col-md-5 col-xs-5">Hotel Discount</div>
                                    <div class="col-md-1 col-xs-1">:</div>
                                    <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal(Model.objBooking.dccDiscount))</strong></div>
                                </div>
                            }

                            if (Model.objBooking.cBookingType == "R")
                            {
                                if (Model.objBooking.bPromoDiscount == true)
                                {
                                    <div class="row">
                                        <div class="col-md-5  col-xs-5">Discount</div>
                                        <div class="col-md-1 col-xs-1">:</div>
                                        <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @OneFineRateAppUtil.clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal(Model.objBooking.dDiscountedBidPrice))</strong></div>
                                    </div>
                                    GrandTotal = clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal((@Model.objBooking.dTotalAmount + @Model.objBooking.dTotalExtraBedAmount + finalTaxes) - Convert.ToDecimal(@Model.objBooking.dDiscountedBidPrice)));
                                }
                                else if (Model.objBooking.bPromoDiscount == false)
                                {
                                    <div class="row">
                                        <div class="col-md-5  col-xs-5">Offer Given</div>
                                        <div class="col-md-1 col-xs-1">:</div>
                                        <div class="col-md-6  col-xs-5  pull-right"><strong> @Model.objBooking.bPromoAmenity</strong></div>
                                    </div>
                                }
                            }

                            <hr>

                            <div class="row">
                                <div class="col-md-5  col-xs-5">Total</div>
                                <div class="col-md-1  col-xs-2">:</div>
                                <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @GrandTotal</strong></div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>

    </div>
