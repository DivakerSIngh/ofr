                                                            <div class="col-md-5 col-xs-5 pull-left">Rooms Rate</div>
                                                            <div class="col-md-1 col-xs-1">:</div>

                                                            @{

                                                                <div class="col-md-6  col-xs-5 pull-right"><strong>@Model.Symbol @clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal(Model.objBooking.dTotalAmount))</strong></div>
                                                                GrandTotal = clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal(@Model.objBooking.dTotalAmount + @Model.objBooking.dTotalExtraBedAmount + finalTaxes));

                                                            }


                                                        </div>

                                                                if (Model.objBooking.dTotalExtraBedAmount != null && Convert.ToDecimal(Model.objBooking.dTotalExtraBedAmount) > 0)
