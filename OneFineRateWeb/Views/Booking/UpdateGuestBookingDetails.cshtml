                                                                        <div class="col-md-1 col-xs-1">:</div>
                                                                        <div class="col-md-6  col-xs-5  pull-right"><strong>@Model.Symbol @clsUtils.ConvertNumberToCommaSeprated(Convert.ToDecimal(Model.objBooking.dTotalExtraBedAmount)) </strong></div>
                                                                    </div>
                                                                    }

                                                                }
                        }

                        @if (Model.objBooking.sExtra3 == "Redeem")
