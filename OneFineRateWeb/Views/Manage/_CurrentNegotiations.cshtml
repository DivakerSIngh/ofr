@model IEnumerable<OneFineRateBLL.Entities.etblBookingTx>

@if (Model != null && Model.Count()> 0)
{
        <div class="col-md-12" >
            <div class="tablebluebg">Your Current Negotiation</div>
            <div class="table table-responsive" >

                <table class="table table-bordered table-hover table-striped">
                    <thead>
                    <th>Hotel Name, City</th>
                    <th>Published Rate</th>
                    <th>Amount</th>
                    <th>Status</th>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.sHotelName, <span class="bluetext">@item.sPropertyCityName</span></td>
                                <td>@item.sTotalAmount</td>
                                <td>@item.sTotalNegotiateAmount </td>
                                <td>@item.BookingStatus</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    
}
else
{
    <div class="row margintop20">
        <div class="col-md-12">
            <div class="tablebluebg">Your Current Negotiation</div>
            <div class="table table-responsive">
                <table class="table table-bordered table-hover table-striped">
                    <tbody>
                        <tr><td colspan="4">No Records found!</td></tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
}