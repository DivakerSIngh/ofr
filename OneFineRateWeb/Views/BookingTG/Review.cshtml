                                }
                                <td data-title="NIGHT(S)">@numberOfNights</td>
                                <td data-title="TOTAL PRICE">@Model.Symbol @(OneFineRateAppUtil.clsUtils.ConvertNumberToCommaSeprated(((pricePerNight * numberOfNights) + (extraBedPricePerNight * numberOfNights)) * ViewBag.dExchangeRate))</td>
                            </tr>

                            if (roomDetail != null)
                            {
                                foreach (var item in roomDetail.RatePlans)
                                {
                                    //TO DO
                                    rateInclusions = item.RateInclusions + Environment.NewLine;
                                    cancellation = Html.Raw(item.CancellationPolicyDescription != null ? item.CancellationPolicyDescription.Replace(".", ".<br/>") : item.CancellationPolicyDescription);
