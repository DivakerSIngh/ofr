@model OneFineRateBLL.Entities.eBookingModify
@using OneFineRateAppUtil
@{
    ViewBag.Title = "Booking Modify/Cancel";
    Layout = "~/Views/Shared/_LayoutIntranet.cshtml";
}
@{
